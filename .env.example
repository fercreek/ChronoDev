# ChronoDev Environment Configuration
# Copy this file to .env and fill in your actual values

# GitHub Configuration
REACT_APP_GITHUB_USERNAME=your_github_username
REACT_APP_GITHUB_TOKEN=ghp_your_github_personal_access_token

# Default repositories to analyze (comma-separated, without username prefix)
# Example: repo1,repo2,repo3
REACT_APP_DEFAULT_REPOS=vayla,docuflow,StudioLinkDesktop

# Optional: Custom API base URL (defaults to https://api.github.com)
# REACT_APP_GITHUB_API_BASE=https://api.github.com

# Development settings
# REACT_APP_DEBUG_MODE=true
